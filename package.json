{
  "name": "caxa",
  "version": "1.0.0",
  "description": "Package Node.js applications into executable binaries",
  "main": "lib/index.js",
  "bin": {
    "caxa": "lib/index.js"
  },
  "types": "lib/index.d.ts",
  "repository": "leafac/caxa",
  "keywords": [
    "packing",
    "deployment",
    "binary"
  ],
  "author": "Leandro Facchinetti <caxa@leafac.com> (https://leafac.com)",
  "license": "MIT",
  "bugs": "https://github.com/leafac/caxa/issues",
  "homepage": "https://github.com/leafac/caxa#readme",
  "scripts": {
    "test": "prettier --check \"src/**/*.ts\" --end-of-line auto && jest",
    "prepare": "tsc",
    "build:stub": "cross-env CGO_ENABLED=0 go build -o stub src/stub.go && shx echo CAXACAXACAXA >> stub"
  },
  "dependencies": {
    "archiver": "^5.2.0",
    "commander": "^7.0.0",
    "crypto-random-string": "^3.3.0",
    "execa": "^4.1.0",
    "fs-extra": "^9.1.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.1.0",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.26",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "jest": {
    "rootDir": "src",
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
